<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
        name="Jira">
    <description>Provides a scanner for Jira.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.jira.model.Jira</class>
        <class>org.jqassistant.contrib.plugin.jira.model.JiraPluginConfigurationFile</class>
        <class>org.jqassistant.contrib.plugin.jira.model.JiraID</class>

        <class>org.jqassistant.contrib.plugin.jira.model.JiraProject</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.jira.scanner.JiraScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>